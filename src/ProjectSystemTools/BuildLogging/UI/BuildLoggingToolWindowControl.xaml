<UserControl x:Class="Microsoft.VisualStudio.ProjectSystem.Tools.BuildLogging.UI.BuildLoggingToolWindowControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:ui="clr-namespace:Microsoft.VisualStudio.ProjectSystem.Tools.BuildLogging.UI"
             Background="{DynamicResource VsBrush.Window}"
             Foreground="{DynamicResource VsBrush.WindowText}"
             mc:Ignorable="d"
             d:DesignHeight="300" d:DesignWidth="300"
             x:Name="BuildLoggingToolWindow"
             x:ClassModifier="internal"
             >
    <Grid>
        <ui:LoggingTreeView x:Uid="TreeView_Logging" x:Name="_loggingTreeView" Style="{DynamicResource {x:Type TreeView}}"/>
    </Grid>
</UserControl>
